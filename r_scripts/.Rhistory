# Aoyagi et al. (1994)
aoyagi <- pwr.t.test(d = 0.42, n = 7, sig.level = 0.05,
type ="one.sample",alternative ="two.sided")
# Heled et al.(2012)
heled <- pwr.t.test(d = 0.42, n = 8, sig.level = 0.05,
type ="one.sample",alternative ="two.sided")
# Lorenzo et al. (2010); assuming equal sample sizes (n = 12 per group)
lorenzo <- wp.rmanova(n = 24, ng = 1, nm = 2, f = 0.21, nscor = 1,
alpha = 0.05, type = 1)
# Machado-Moreira et al. (2019)
machado <- pwr.t.test(d = 0.42, n = 6, sig.level = 0.05,
type ="one.sample",alternative ="two.sided")
# Neal et al. (2016) [2]
neal <- pwr.t.test(d = 0.42, n = 10, sig.level = 0.05,
type ="one.sample",alternative ="two.sided")
# Pivarnik et al. (1987)
pivarnik <- wp.rmanova(n = 16, ng = 1, nm = 2, f = 0.21, nscor = 1,
alpha = 0.05, type = 1)
# Sawka et al. (1985)
sawka <- pwr.t.test(d = 0.42, n = 13, sig.level = 0.05,
type ="one.sample",alternative ="two.sided")
# Sotiridis et al. (2018)
sotiridis <- wp.rmanova(n = 12, ng = 3, nm = 2, f = 0.21, nscor = 1,
