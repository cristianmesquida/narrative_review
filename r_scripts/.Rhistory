# Mikkelsen et al. (2019)
mikkelsen <- wp.rmanova(n = 24, ng = 2, nm = 2, f = 0.15, nscor = 1,
alpha = 0.05, type = 0)
# Neal et al. (2016) [1]
neal <- wp.rmanova(n = 8, ng = 2, nm = 2, f = 0.15, nscor = 1,
alpha = 0.05, type = 0)
# Rendell et al. (2020)
rendell <- wp.rmanova(n = 8, ng = 2, nm = 2, f = 0.15, nscor = 1,
alpha = 0.05, type = 0)
# Rivas et al. (2017)
rivas <- wp.rmanova(n = 8, ng = 2, nm = 2, f = 0.15, nscor = 1,
alpha = 0.05, type = 0)
# Ronnestad et al. (2020); assuming equal sample sizes (n = 11 per group)
ronnestad <- wp.rmanova(n = 22, ng = 2, nm = 2, f = 0.15, nscor = 1,
